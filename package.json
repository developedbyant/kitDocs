		"svelte": "^5.0.0-next.51",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"create-svelte": "^5.3.4",
		"fs-extra": "^11.2.0",
		"globby": "^13.2.2",
		"inquirer": "^9.2.14",
