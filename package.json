{
	"name": "kitdocs",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"testApp": "vite build && vite preview",
		"package": "node package/build.js && cd ./package && npm publish && cd ..",
		"buildApp": "vite build",
		"startApp": "PORT=3002 pm2 start ./build/index.js --name=kitdocs.com"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.30.3",
		"@types/node": "^20.11.16",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.3",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^4.5.2"
	},
	"type": "module",
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"create-svelte": "^5.3.4",
		"fs-extra": "^11.2.0",
		"globby": "^13.2.2",
		"inquirer": "^9.2.14",
		"marked": "^12.0.0",
		"shiki": "^0.14.7"
	}
}
